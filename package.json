{
  "name": "table-react-component-library",
  "version": "1.0.9",
  "description": "A table that can sort, search and display data with pagination",
  "keywords": [
    "table",
    "react",
    "sort",
    "filter",
    "search"
  ],
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/DataTable.umd.js",
  "module": "./dist/DataTable.es.js",
  "exports": {
    ".": {
      "import": "./dist/DataTable.es.js",
      "require": "./dist/DataTable.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarieRodiet/table-react-component-library.git"
  },
  "author": "Marie Moore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarieRodiet/table-react-component-library/issues"
  },
  "homepage": "https://github.com/MarieRodiet/table-react-component-library#readme",
  "devDependencies": {
    "@testing-library/dom": "^8.18.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^2.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-plugin-react": "^7.31.8",
    "jsdom": "^20.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "vite-plugin-svgr": "^2.2.1"
  }
}
